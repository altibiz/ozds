@using Color = MudBlazor.Color
<MudDialog>
  <DialogContent>
    Success
  </DialogContent>
  <DialogActions>
    <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
  </DialogActions>
</MudDialog>

@code {

  [CascadingParameter]
  MudDialogInstance MudDialog { get; set; } = default!;

  void Submit()
  {
    MudDialog.Close(DialogResult.Ok(true));
  }

}
