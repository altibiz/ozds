@using Ozds.Business.Models.Base
@using Ozds.Business.Models

@namespace Ozds.Client.Components.Models

@if (Model is NetworkUserMeasurementLocationModel networkUserMeasurementLocationModel)
{
  <NetworkUserMeasurementLocationEdit Model="networkUserMeasurementLocationModel"/>
}
else if (Model is LocationMeasurementLocationModel locationMeasurementLocationModel)
{
  <LocationMeasurementLocationEdit Model="locationMeasurementLocationModel"/>
}

@code {
  [Parameter]
  public MeasurementLocationModel Model { get; set; } = default!;
}
