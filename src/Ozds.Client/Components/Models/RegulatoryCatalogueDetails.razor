
<DetailsField Title="@T["Name"]">
  @Model.Title
</DetailsField>
<DetailsField Title="@T["Price of higher tariff active energy consumption (EUR/kWh)"]">
  @Model.ActiveEnergyTotalImportT1Price_EUR
</DetailsField>
<DetailsField Title="@T["Price of active energy consumption in low tariff (EUR/kWh)"]">
  @Model.ActiveEnergyTotalImportT2Price_EUR
</DetailsField>
<DetailsField Title="@T["Price of personal use fee (EUR/kWh)"]">
  @Model.RenewableEnergyFeePrice_EUR
</DetailsField>
<DetailsField Title="@T["Price of business use fee (EUR/kWh)"]">
  @Model.BusinessUsageFeePrice_EUR
</DetailsField>
<DetailsField Title="@T["Tax (%)"]">
  @Model.TaxRate_Percent
</DetailsField>


@code {

  [Parameter]
  public RegulatoryCatalogueModel Model { get; set; } = default!;

}
