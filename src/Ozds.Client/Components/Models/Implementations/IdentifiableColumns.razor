@using MudBlazor
@using Ozds.Business.Models.Abstractions
@using Ozds.Client.Components.Models.Base

@namespace Ozds.Client.Components.Models

@typeparam TModel where TModel : IIdentifiable
@inherits OzdsColumnsComponentBase<TModel>

<TemplateColumn T="TModel">
  <CellTemplate>
    <MudLink Href="@Cache.CreateLink(context.Item)">
      <MudText>
        @context.Item.Title
      </MudText>
    </MudLink>
  </CellTemplate>
</TemplateColumn>
