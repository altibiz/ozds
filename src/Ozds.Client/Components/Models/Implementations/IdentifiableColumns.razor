@using MudBlazor
@using Ozds.Business.Models.Abstractions
@using Ozds.Client.Components.Models.Base

@namespace Ozds.Client.Components.Models

@typeparam TPrefix
@typeparam TModel where TModel : IIdentifiable
@inherits OzdsColumnsComponentBase<TPrefix, TModel>

<TemplateColumn T="TPrefix">
  <CellTemplate>
    <MudLink Href="@(Cache.CreateLink(Fix(context.Item)))">
      <MudText>
        @(Fix(context.Item).Title)
      </MudText>
    </MudLink>
  </CellTemplate>
</TemplateColumn>
