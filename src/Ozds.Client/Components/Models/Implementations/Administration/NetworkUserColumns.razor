@using MudBlazor
@using Ozds.Business.Models
@using Ozds.Business.Models.Abstractions
@using Ozds.Client.Components.Models.Base

@namespace Ozds.Client.Components.Models.Implementations.Administration

@typeparam TModel where TModel : NetworkUserModel
@inherits OzdsColumnsComponentBase<TModel>

@(Base<IIdentifiable>())

<PrefixedColumns Prefix="@(x => x.LegalPerson)" Models="@Models" />

<PropertyColumn
  T="TModel"
  TProperty="string"
  Property="x => x.AltiBizSubProjectCode"
  Title='@Translate("AltiBiz subproject code")'>
  <CellTemplate>
    <MudText>
      @context.Item.AltiBizSubProjectCode
    </MudText>
  </CellTemplate>
</PropertyColumn>
