@using Microsoft.AspNetCore.Components
@using MudBlazor
@using Ozds.Business.Models
@using Ozds.Business.Models.Abstractions

@namespace Ozds.Client.Components.Models

@if (Model is AbbB2xMeterModel abbB2XMeterModel) {
  <AbbB2xMeterSummary Model="abbB2XMeterModel"/>
}
else if (Model is SchneideriEM3xxxMeterModel schneideriEM3XxxMeterModel) {
  <SchneideriEM3xxxMeterSummary Model="schneideriEM3XxxMeterModel"/>
}
else{
  throw new ArgumentOutOfRangeException(nameof(Model));
}

@code {
  [Parameter]
  public IMeter Model { get; set; } = default!;
}
