@using MudBlazor
@using Ozds.Business.Models.Abstractions
@using Ozds.Client.Components.Models.Base

@typeparam TPrefix
@typeparam TModel where TModel : IMeasurementValidator
@inherits OzdsColumnsComponentBase<TPrefix, TModel>

<PropertyColumn
  T="TPrefix"
  TProperty="string"
  Property="x => Fix(x)!.Title"
  Title='@Translate("Title")'>
  <CellTemplate>
    <MudText>@Fix(context.Item)?.Title</MudText>
  </CellTemplate>
</PropertyColumn>
