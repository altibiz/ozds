@using Ozds.Business.Models
@using Ozds.Business.Models.Base
@using Ozds.Client.Components.Models.Base

@namespace Ozds.Client.Components.Models

@typeparam TPrefix
@typeparam TModel where TModel : AbbB2xMeasurementValidatorModel
@inherits OzdsEditComponentBase<TPrefix, TModel>

@(Base<IdentifiableModel>())

@Field(x => x.MinVoltage_V)
