@using Ozds.Business.Models.Abstractions
@using Ozds.Business.Models

@using Ozds.Client.Components.Models.Base

@namespace Ozds.Client.Components.Models

@inherits OzdsSummaryComponentBase<ICalculation>

@if (Model is BlueLowNetworkUserCalculationModel blueLowNetworkUserCalculation)
{
  <BlueLowNetworkUserCalculationSummary Model="blueLowNetworkUserCalculation"/>
}
else if (Model is RedLowNetworkUserCalculationModel redLowNetworkUserCalculation)
{
  <RedLowNetworkUserCalculationSummary Model="redLowNetworkUserCalculation"/>
}
else if (Model is WhiteLowNetworkUserCalculationModel whiteLowNetworkUserCalculation)
{
  <WhiteLowNetworkUserCalculationSummary Model="whiteLowNetworkUserCalculation"/>
}
else if (Model is WhiteMediumNetworkUserCalculationModel whiteMediumNetworkUserCalculation)
{
  <WhiteMediumNetworkUserCalculationSummary Model="whiteMediumNetworkUserCalculation"/>
}

@code {}
