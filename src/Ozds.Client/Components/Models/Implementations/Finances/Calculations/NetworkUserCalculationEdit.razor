@typeparam TPrefix
@typeparam TModel where TModel : Ozds.Business.Models.Base.NetworkUserCalculationModel
@using Ozds.Business.Models.Base
@using Ozds.Client.Components.Streaming
@inherits Ozds.Client.Components.Models.Base.OzdsEditComponentBase<TPrefix, TModel>

@(Base<IdentifiableModel>())

@(
Base<FinancialModel>(
  @<Fragment>
    @Sub()

    <PrefixedEdit Model="@Model" Prefix="@Wrap(x => x.UsageMeterFee)"/>

    @Field(x => x.UsageFeeTotal_EUR)

    <PrefixedEdit
      Model="@Model"
      Prefix="@Wrap(x => x.SupplyActiveEnergyTotalImportT1)"/>

    <PrefixedEdit
      Model="@Model"
      Prefix="@Wrap(x => x.SupplyActiveEnergyTotalImportT2)"/>

    <PrefixedEdit Model="@Model" Prefix="@Wrap(x => x.SupplyBusinessUsageFee)"/>

    <PrefixedEdit Model="@Model" Prefix="@Wrap(x => x.SupplyRenewableEnergyFee)"/>

    @Field(x => x.SupplyFeeTotal_EUR)
  </Fragment>))
