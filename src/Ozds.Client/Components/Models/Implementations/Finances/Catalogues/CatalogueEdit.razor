@using Microsoft.AspNetCore.Components
@using MudBlazor
@using Ozds.Business.Models
@using Ozds.Business.Models.Base
@using Ozds.Business.Models.Abstractions

@namespace Ozds.Client.Components.Models

@if(Model is BlueLowNetworkUserCatalogueModel blueLowNetworkUserCatalogueModel){
  <BlueLowNetworkUserCatalogueEdit Model="blueLowNetworkUserCatalogueModel"/>
}
else if(Model is RedLowNetworkUserCatalogueModel redLowNetworkUserCatalogueModel){
  <RedLowNetworkUserCatalogueEdit Model="redLowNetworkUserCatalogueModel"/>
}
else if(Model is WhiteLowNetworkUserCatalogueModel whiteLowNetworkUserCatalogueModel){
  <WhiteLowNetworkUserCatalogueEdit Model="whiteLowNetworkUserCatalogueModel"/>
}
else if(Model is WhiteMediumNetworkUserCatalogueModel whiteMediumNetworkUserCatalogueModel){
  <WhiteMediumNetworkUserCatalogueEdit Model="whiteMediumNetworkUserCatalogueModel"/>
}
else if(Model is RegulatoryCatalogueModel regulatoryCatalogueeModel){
  <RegulatoryCatalogueEdit Model="regulatoryCatalogueeModel"/>
}
else{
  throw new ArgumentOutOfRangeException(nameof(Model));
}

@code {
  [Parameter]
  public ICatalogue Model { get; set; } = default!;
}
