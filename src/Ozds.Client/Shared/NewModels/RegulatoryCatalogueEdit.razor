
<MudTextField
  Label="@T["Name"]"
  @bind-Value="Model.Title"
  For="@(() => Model.Title)"/>
<MudTextField
  T="decimal"
  Label="@T["Price of higher tariff active energy consumption (EUR/kWh)"]"
  @bind-Value="Model.ActiveEnergyTotalImportT1Price_EUR"
  For="@(() => Model.ActiveEnergyTotalImportT1Price_EUR)"/>
<MudTextField
  T="decimal"
  Label="@T["Price of active energy consumption in low tariff (EUR/kWh)"]"
  @bind-Value="Model.ActiveEnergyTotalImportT2Price_EUR"
  For="@(() => Model.ActiveEnergyTotalImportT2Price_EUR)"/>
<MudTextField
  T="decimal"
  Label="@T["Price of personal use fee (EUR/kWh)"]"
  @bind-Value="Model.RenewableEnergyFeePrice_EUR"
  For="@(() => Model.RenewableEnergyFeePrice_EUR)"/>
<MudTextField
  T="decimal"
  Label="@T["Price of business use fee (EUR/kWh)"]"
  @bind-Value="Model.BusinessUsageFeePrice_EUR"
  For="@(() => Model.BusinessUsageFeePrice_EUR)"/>
<MudTextField
  Label="@T["Tax (%)"]"
  @bind-Value="Model.TaxRate_Percent"
  For="@(() => Model.TaxRate_Percent)"/>

@code {

  [Parameter]
  public RegulatoryCatalogueModel Model { get; set; } = default!;

}
