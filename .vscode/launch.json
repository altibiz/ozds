{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug-server",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/src/Ozds.Server/bin/Debug/net8.0/Ozds.Server.dll",
      "preLaunchTask": "build-server",
      "cwd": "${workspaceFolder}/src/Ozds.Server",
      "stopAtEntry": false,
      "requireExactSource": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "ORCHARD_APP_DATA": "${workspaceFolder}/App_Data"
      },
      "logging": {
        "moduleLoad": false
      }
    },
    {
      "name": "debug-server-external",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/src/Ozds.Server/bin/Debug/net8.0/Ozds.Server.dll",
      "preLaunchTask": "build-server",
      "cwd": "${workspaceFolder}/src/Ozds.Server",
      "stopAtEntry": false,
      "requireExactSource": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "ORCHARD_APP_DATA": "${workspaceFolder}/App_Data"
      },
      "logging": {
        "moduleLoad": false
      },
      "justMyCode": false,
      "symbolOptions": {
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      }
    },
    {
      "name": "debug-fake",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/Ozds.Fake/bin/Debug/net8.0/Ozds.Fake.dll",
      "preLaunchTask": "build-fake",
      "cwd": "${workspaceFolder}/scripts/Ozds.Fake",
      "stopAtEntry": false,
      "requireExactSource": false,
      "args": [
        "--timeout",
        "1",
        "--interval",
        "3",
        "--meter-ids",
        "abb-B2x-1624226",
        "schneider-iEM3xxx-19453067"
      ],
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development"
      },
      "logging": {
        "moduleLoad": false
      }
    },
    {
      "name": "debug-fake-external",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/Ozds.Fake/bin/Debug/net8.0/Ozds.Fake.dll",
      "preLaunchTask": "build-fake",
      "cwd": "${workspaceFolder}/scripts/Ozds.Fake",
      "stopAtEntry": false,
      "requireExactSource": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development"
      },
      "logging": {
        "moduleLoad": false
      },
      "justMyCode": false,
      "symbolOptions": {
        "searchMicrosoftSymbolFake": true,
        "searchNuGetOrgSymbolFake": true
      }
    }
  ]
}
